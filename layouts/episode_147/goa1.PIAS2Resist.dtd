      <!ELEMENT ID (#PCDATA)>
      <!ELEMENT Part_of_blast_panel_ID (#PCDATA)>
      <!ELEMENT Contour_vertex_IDs (Contour_vertex_ID+)>
        <!ELEMENT Contour_vertex_ID (#PCDATA)>
