          <!ELEMENT L (#PCDATA)>
          <!ELEMENT B (#PCDATA)>
          <!ELEMENT H (#PCDATA)>
      <!ELEMENT Resist_vertices (Resist_vertex+)>
        <!ELEMENT Resist_vertex (L,B,H)>
          <!ELEMENT L (#PCDATA)>
          <!ELEMENT B (#PCDATA)>
          <!ELEMENT H (#PCDATA)>
