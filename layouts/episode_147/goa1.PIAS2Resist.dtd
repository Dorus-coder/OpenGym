      <!ELEMENT Constituent_fragment_panel_IDs (Constituent_fragment_panel_ID+)>
        <!ELEMENT Constituent_fragment_panel_ID (#PCDATA)>
  <!ELEMENT Compartments (Compartment+)>
    <!ELEMENT Compartment (Compartment_GUID+,Name+,Maximum_volume+,Floor_area+,Bounding_blast_panel_IDs,Bounding_fragment_panel_IDs)>
